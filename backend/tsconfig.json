{
    "compilerOptions": {
      "target": "ES2022",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
  
      "rootDir": "./",
      "outDir": "./dist",
  
      "strict": true,
      "skipLibCheck": true,
  
      "noEmitOnError": false,
      "esModuleInterop": true,
  
      "forceConsistentCasingInFileNames": true
    }
  }
  